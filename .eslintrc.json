{
    "env": {
      "commonjs": true,
      "node": true,
      "es2021": true
    },
    "parser": "@babel/eslint-parser",
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-useless-catch": "off",
      "no-console": ["error", { "allow": ["warn", "error"] }]
    }
}
